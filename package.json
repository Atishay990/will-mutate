{
	"name": "will-mutate",
	"version": "1.0.0",
	"description": "Runtime test to that detects mutations to objects",
	"keywords": [
		"babel-plugin",
		"mutation",
		"testing",
		"proxy"
	],
	"homepage": "https://github.com/ceoss/will-mutate#readme",
	"bugs": {
		"url": "https://github.com/ceoss/will-mutate/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ceoss/will-mutate.git"
	},
	"license": "MIT",
	"author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
	"contributors": [
		"Corbin Crutchley <crutchcorn@gmail.com> (https://crutchcorn.dev)"
	],
	"main": "plugin/index.js",
	"files": [
		"plugin/",
		"utils/",
		"noop/",
		"!**/__tests__/",
		"!**/__mocks__/",
		"!**/__fixtures__/"
	],
	"scripts": {
		"format": "eslint --fix ./",
		"lint": "eslint ./",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": "./node_modules/conventional-changelog-evelyn/commitlint.config.js"
	},
	"lint-staged": {
		"**/package.json": "sort-package-json",
		"./": "eslint --fix"
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/prompt"
		}
	},
	"dependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/parser": "^7.10.5",
		"@babel/traverse": "^7.10.5",
		"@babel/types": "^7.10.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.10.5",
		"@commitlint/prompt": "^9.1.1",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"babel-plugin-tester": "^9.2.0",
		"babel-test": "^0.2.3",
		"commitlint": "^9.1.0",
		"conventional-changelog-evelyn": "^1.2.0",
		"eslint": "^7.5.0",
		"eslint-plugin-evelyn": "^3.1.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jest": "^23.18.0",
		"eslint-plugin-mocha": "^7.0.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-unicorn": "^21.0.0",
		"husky": "^4.2.5",
		"jest": "^26.1.0",
		"jest-file-snapshot": "^0.3.8",
		"lint-staged": "^10.2.11",
		"semantic-release": "^17.1.1",
		"sort-package-json": "^1.44.0"
	},
	"peerDependencies": {
		"@babel/core": "^7.0.0"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"release": {
		"extends": "conventional-changelog-evelyn/release.config.js"
	}
}
